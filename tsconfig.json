{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "components/*": ["app/components/*"],
      "lib/*": ["app/lib/*"],
      "hook/*": ["app/hooks/*"],
      "modals/*": ["app/modals/*"],
      "app/*": ["app/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
